{
  "compilerOptions": {
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "experimentalDecorators": true,
    "importHelpers": false,
    "noEmit": true,
    "removeComments": true,
    "module": "CommonJS",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "incremental": true,
    "moduleResolution": "node",
    "lib": [
      "DOM",
      "ES2020"
    ],
    "target": "es6",
    "sourceMap": true,
    "jsx": "react-jsx"
  },
  "exclude": [
    "dist"
  ]
}